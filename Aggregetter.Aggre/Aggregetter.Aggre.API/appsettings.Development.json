{
  "ConnectionStrings": {
    "AggreConnectionString": "Server=localhost;Database=Aggregetter.Data;Uid=sa;Pwd=MyPassword;",
    "AggreIdentityConnectionString": "Server=localhost;Database=Aggregetter.Identity;Uid=sa;Pwd=MyPassword;",
    "MySQLVersion": "8.0.0",
    "RedisConnectionString": "",
    "RabbitMQConnectionString": "localhost"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "ApplicationName": "Serilog.WebApplication"
    }
  },
  "JwtSettings": {
    "Issuer": "AggreIdentity",
    "Audience": "AggreIdentityUser",
    "DurationInMinutes": 1,
    "Key": "ThisIsNotASecretKey"
  }
}
